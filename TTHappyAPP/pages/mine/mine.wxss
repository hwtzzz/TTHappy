/* pages/set/set.wxss */
.box {
    width: 350px;
    height: 680px;
    margin: 0 auto;
    margin-top: 10px;
}
.setList {
    height: 250px;
    width: 100%;
    margin-top: 20px;
}

.list {
    height: 50px;
    width: 100%;
    border-bottom: 1px solid rgb(241, 238, 238);
}

.list image {
    width: 24px;
    height: 24px;
    margin-top: 13px;
    float: left;
}

.setTitle {
    float: left;
    margin-left: 15px;
    line-height: 50px;
    font-size: 14px;
    font-weight: 600;
}

.tuichuBox {
    height: 40px;
    width: 100%;
    margin-top: 20px;
    color: #fff;
    float: left;
    text-align: center;
    font-size: 15px;
    font-weight: 600;
    line-height: 40px;
    background-color: #49a7d8;
    border-radius: 20px;
}